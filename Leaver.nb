Cell[TextData[{
 "Schwarzschild eqn. for perturbations is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"r", "(", 
       RowBox[{"r", "-", "1"}], ")"}], 
      SubscriptBox["\[Psi]", 
       RowBox[{"l", ",", "rr"}]]}], "+", 
     SubscriptBox["\[Psi]", 
      RowBox[{"l", ",", "r"}]], "-", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SuperscriptBox["p", "2"], 
         FractionBox[
          SuperscriptBox["r", "3"], 
          RowBox[{"r", "-", "1"}]]}], "+", 
        RowBox[{"l", "(", 
         RowBox[{"l", "+", "1"}], ")"}], "-", 
        FractionBox["\[Epsilon]", "r"]}], ")"}], 
      SubscriptBox["\[Psi]", "l"]}]}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"e7d36af2-d75f-49e3-9fa3-7a01477b5092"],
 ", where 2M=G=c=1"
}], "Item",
